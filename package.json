{
  "name": "weatherwidget",
  "version": "0.1.0",
  "description": "Widget to get current weather conditions of a given location",
  "devDependencies": {
    "browser-sync": "1.8.2",
    "browserify": "^9.0.7",
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.5.2",
    "gulp-jshint": "^1.10.0",
    "gulp-less": "^3.0.2",
    "gulp-minify-css": "^1.0.0",
    "gulp-run": "^1.6.7",
    "gulp-uglify": "1.0.2",
    "reactify": "^1.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "paths": {
    "less": "assets/less/*.less",
    "js": "./app/**/*.js",
    "jsx": "./app/**/*.jsx",
    "app": "./app/weatherwidget.js",
    "html": "*.html"
  },
  "dest": {
    "style": "style.css",
    "app": "weatherwidget.js",
    "dist": "dist"
  }
}
